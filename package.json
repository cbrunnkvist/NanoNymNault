{
  "name": "nault",
  "version": "1.18.3",
  "license": "MIT",
  "description": "Wallet for interacting with nano",
  "author": "Conny Brunnkvist <cbrunnkvist@gmail.com>",
  "main": "desktop-app/dist/desktop-app.js",
  "scripts": {
    "ng": "npm run version && ng",
    "start": "npm run ng -- serve",
    "build": "npm run version && ng build",
    "test": "npm run ng -- test --watch=false",
    "test:watch": "npm run ng -- test",
    "lint": "npm run ng -- lint",
    "e2e": "npm run ng -- e2e",
    "publish": "build -wml --x64 -p always",
    "release": "npm run desktop:build && electron-builder",
    "wallet:dev": "npm run ng -- serve --open",
    "wallet:dev-ssl": "npm run ng -- serve --ssl --open",
    "version": "node scripts/generate-version.js",
    "wallet:build": "npm run ng -- build --configuration production --base-href /NanoNymNault/",
    "wallet:build-desktop": "npm run ng -- build --configuration=desktop --base-href ",
    "desktop:compile": "cd desktop-app && tsc && cd ..",
    "desktop:build": "npm run wallet:build-desktop && npm run desktop:compile",
    "desktop:build-local": "npm run desktop:build && npm run desktop:local",
    "desktop:dev": "npm run desktop:compile && electron desktop-app/dist/desktop-app.js",
    "desktop:local": "electron-builder -p never"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "17.3.17",
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/localize": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@angular/service-worker": "^17.3.12",
    "@chainsafe/libp2p-gossipsub": "^13.2.0",
    "@ledgerhq/hw-transport": "^6.27.1",
    "@ledgerhq/hw-transport-node-ble": "^6.27.1",
    "@ledgerhq/hw-transport-node-hid": "^6.27.1",
    "@ledgerhq/hw-transport-u2f": "^5.34.0",
    "@ledgerhq/hw-transport-web-ble": "^6.27.1",
    "@ledgerhq/hw-transport-webhid": "^6.27.1",
    "@ledgerhq/hw-transport-webusb": "^6.27.1",
    "@ledgerhq/logs": "^6.10.0",
