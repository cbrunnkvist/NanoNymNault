    "@orbitdb/core": "^3.0.2",
    "@popperjs/core": "^2.11.8",
    "@types/crypto-js": "^3.1.38",
    "@types/w3c-web-hid": "^1.0.2",
    "@types/w3c-web-usb": "^1.0.5",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "@zxing/ngx-scanner": "^17.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bignumber.js": "^5.0.0",
    "bip39": "^3.0.2",
    "blakejs": "^1.1.0",
    "blockstore-idb": "^3.0.1",
